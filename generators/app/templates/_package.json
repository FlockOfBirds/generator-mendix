{
  "name": "<%- packageName %>",
  "widgetName": "<%- widgetName %>",
  "version": "<%- version %>",
  "description": "<%- description %>",
  "copyright": "<%- copyright %>",
  "author": "<%- author %>",
  "config": { "widgetPath": "./dist/MxTestProject/widgets" },
  "scripts": {<% if (builder === "grunt") { %>
    "start": "nodemon --watch webpack.config.js --watch Gruntfile.js --exec \"grunt",<% } %>
    "start:dev": "webpack-dev-server --hot --open",
    "emit": "tsc",<% if (unitTests || e2eTests) { %>
    "pretest": "tsc && npm run lint && npm run build",<% } %><% if (unitTests) { %>
    "test": "npm run test:unit && npm run test:conditional",
    "test:conditional": "node ./node_modules/mendix-widget-build-script/dist/CheckPullRequestFromFork.js && echo 'Skip tests on forked pull request' || npm run deployAndTest",
    "test:dev": "karma start",
    "test:unit": "karma start --single-run --codeCoverage && node tests/remap.js",<% } %><% if (e2eTests) { %>
    "test:e2e": "tsc -p ./tests/e2e/tsconfig.json && wdio ./tests/e2e/wdio.conf.js",
    "test:e2e:dev": "tsc -p ./tests/e2e/tsconfig.json && set DEBUG=true&& wdio ./tests/e2e/wdio.conf.js",<% } %>
    "lint": "tslint -c tslint.json '**/*.ts' '**/*.tsx' --exclude '**/node_modules/**'",<% if (builder === "grunt") { %>
    "build": "grunt release",<% } if (builder === "gulp") { %>
    "build": "node ./node_modules/gulp/bin/gulp build",
    "version": "node ./node_modules/gulp/bin/gulp version",
    "icon": "node ./node_modules/gulp/bin/gulp icon",
    "folders": "node ./node_modules/gulp/bin/gulp folders",
    "modeler": "node ./node_modules/gulp/bin/gulp modeler"<% } %>
    "lint:fix": "npm run lint -- --fix",
    "deploy": " npm run updateProject && npm run deployApp",
    "updateProject": "node ./node_modules/mendix-widget-build-script/dist/UpdateMxProject.js",
    "deployApp": "node ./node_modules/mendix-widget-build-script/dist/DeployMxApp.js"<% if (e2eTests) { %>,
    "deployAndTest": "npm run deploy && npm run test:e2e"<% } %>
  },
  "pre-commit": [
    "emit",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/<%- author %>/<%- packageName %>"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/classnames": "^2.2.4",
    "@types/core-js": "^2.5.0",<% if (e2eTests) { %>
    "@types/webdriverio": "^4.10.2",<% } %><% if (unitTests || e2eTests) { %>
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",<% } %>
    "@types/globalize": "0.0.34",<% if (unitTests) { %>
    "@types/jasmine": "^2.8.9",
    "@types/karma": "^3.0.0",<% } %>
    "@types/node": "^10.11.7",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "babel-loader": "^8.0.4",
    "check-dependencies": "^1.1.0",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",<% if (unitTests || e2eTests) { %>
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.1.1",<% } %>
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.10",<% if (builder === "grunt") { %>
    "grunt": "^1.0.3",
    "grunt-check-dependencies": "^1.0.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.1",
    "grunt-file-append": "0.0.7",
    "grunt-newer": "^1.3.0",
    "grunt-webpack": "^3.1.2",<% } if (builder === "gulp") { %>
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.1",
    "gulp-intercept": "^0.1.0",
    "gulp-json-transform": "^0.4.2",
    "gulp-newer": "^1.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.7",
    "gulp-zip": "^3.2.0",
    "gulp-jsvalidate": "^3.0.0",
    "yargs": "^6.0.0"<% } %>
    "istanbul": "^0.4.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "json-loader": "^0.5.4",
    "json-source-map": "^0.4.0",<% if (unitTests || e2eTests) { %>
    "jasmine": "^3.2.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",<% } %>
    "mendix-client": "^7.15.3",<% if (e2eTests) { %>
    "mendix-widget-build-script": "^1.0.0",<% } %>
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.3.2",
    "remap-istanbul": "^0.11.1",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.2.0",
    "typescript": "^3.1.2",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.1.2",<% if (e2eTests) { %>
    "wdio-dot-reporter": "0.0.10",
    "wdio-jasmine-framework": "^5.0.0-beta.3",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.8.1",<% } %>
    "webpack": "^4.8.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "webworkify-webpack": "^2.1.3"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
